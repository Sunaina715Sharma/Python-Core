   "source": [
    "try:\n",
    "    with open(\"abc.txt\",\"w\") as f:\n",
    "        print(f.read())\n",
    "except IOError:\n",
    "    # it will only check for IoError exception and then print()\n",
    "    print(\"Error: this file has some error \")\n",
    "except TypeError:\n",
    "    print(\" Error : Thers some error \")\n",
    "else:\n",
    "    print(\"content written suucesfully\")\n",
    "finally:\n",
    "    print(\" i will execute\")\n",
    "    "
